PRINT "=== Test 2: Döngüler ==="

PRINT "FOR döngüsü (1'den 5'e):"
FOR i = 1 TO 5
    PRINT "i =", i
NEXT

PRINT "FOR döngüsü STEP 2 ile (0'dan 10'a):"
FOR j = 0 TO 10 STEP 2
    PRINT "j =", j
NEXT

PRINT "WHILE döngüsü:"
LET counter = 1
WHILE counter <= 3
    PRINT "Counter:", counter
    LET counter = counter + 1
WEND

PRINT "İç içe döngüler - 3x3 tablo:"
FOR row = 1 TO 3
    FOR col = 1 TO 3
        PRINT row * col
    NEXT
NEXT

PRINT "=== Test 2 Tamamlandı ==="
